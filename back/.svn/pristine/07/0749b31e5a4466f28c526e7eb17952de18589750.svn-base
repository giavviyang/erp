import request from '@/utils/request'
/* 查询原片类型列表 */
export function queryTree() {
  return request({
    url: '/accessories/definition/queryTree',
    method: 'post'
  })
}
// 新增原片类型
export function addAccessoriesType(data) {
  return request({
    url: '/accessories/definition/addAccessoriesType',
    method: 'post',
    data: data
  })
}
// 编辑原片类型
export function updAccessoriesType(data) {
  return request({
    url: '/accessories/definition/updAccessoriesType',
    method: 'post',
    data: data
  })
}
// 删除原片类型
export function delAccessoriesType(data) {
  return request({
    url: '/accessories/definition/delAccessoriesType',
    method: 'post',
    params: data
  })
}
