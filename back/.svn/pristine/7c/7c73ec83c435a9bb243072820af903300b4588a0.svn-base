import request from '@/utils/request'

/* 生成采购编号 */
export function productionNumber() {
  return request({
    url: '/accessories/damage/productionNumber',
    method: 'post'
  })
}

/* 查询报损单 */
export function queryDamageData(data) {
  return request({
    url: '/accessories/damage/queryDamageData',
    method: 'post',
    data: data
  })
}

/* 查询明细 */
export function viewDetail(data) {
  return request({
    url: '/accessories/damage/viewDetail',
    method: 'post',
    params: data
  })
}

