import request from '@/utils/request'


/* 生成采购编号 */
export function productionNumber(data) {
  return request({
    url: '/accessories/operation/productionNumber',
    method: 'post',
    params: data
  })
}

/* 辅料出-入-盘库 */
export function accessoriesWarehouse(data) {
  return request({
    url: '/accessories/operation/accessoriesWarehouse',
    method: 'post',
    data: data
  })
}

