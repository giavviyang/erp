import request from '@/utils/request'
/* 查询原片类型列表 */
export function queryTree() {
  return request({
    url: '/originalFilm/definition/queryTree',
    method: 'post'
  })
}
// 新增原片类型
export function addOriginalType(data) {
  return request({
    url: '/originalFilm/definition/addOriginalType',
    method: 'post',
    data: data
  })
}
// 编辑原片类型
export function updOriginalType(data) {
  return request({
    url: '/originalFilm/definition/updOriginalType',
    method: 'post',
    data: data
  })
}
// 删除原片类型
export function delOriginalType(data) {
  return request({
    url: '/originalFilm/definition/delOriginalType',
    method: 'post',
    params: data
  })
}
