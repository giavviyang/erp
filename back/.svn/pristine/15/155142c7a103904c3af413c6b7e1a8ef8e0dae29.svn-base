import request from '@/utils/request'

// 获取发货编号
export function productionNumber() {
  return request({
    url: '/sales/outsourced/productionNumber',
    method: 'post',
  })
}

// 查询外协记录
export function queryOutsourced(data) {
  return request({
    url: '/sales/outsourced/queryOutsourced',
    method: 'post',
    data: data
  })
}

