//表单+按钮
.iptAndBtn {
  background-color: #f6f6f6;
  height: 48px;
  align-items: center;
  display: flex;
  padding-left: 10px;
  width: 100%;
  overflow: auto;

  .el-form-item {
    //width: 260px;
    min-width: 200px;
    margin-right: 0;
    margin-bottom: 0;
    white-space: nowrap;

    .el-form-item__content {
      max-width: 160px;
      margin-right: 10px;
    }
  }

  .daterange {
    min-width: 320px;

    .el-range-editor {
      width: 240px;

      .el-range-separator {
        display: inline-block;
        width: 20px;
      }

      .el-range-input:last-of-type {
        margin-left: 5px;
      }
    }
  }

  .back {
    //margin-right: 10px;
    background-color: #1890ff;
    color: #fff;
  }
}

//纯按钮
.btn {
  height: 48px;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  overflow: auto;
  //padding-right: 100px;
  box-shadow: none;

  .el-button {
    margin: 0 10px 0 5px;
  }
}

