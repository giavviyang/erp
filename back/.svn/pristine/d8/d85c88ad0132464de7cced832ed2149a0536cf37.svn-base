//纯表单1  弹窗
.ipt {
  max-height: calc(100vh - 190px);
  width: 98%;
  margin: 0 auto;
  min-height: 100px;
  overflow: auto;
  display: flex;
  flex-wrap: wrap;
  //justify-content: space-around;

  .el-form-item {
    min-width: calc(50% - 10px);
    //width: 300px;
    display: flex;
    height: 50px;
    justify-content: center;
    align-items: center;
    margin-bottom: 0;

    .el-form-item__label {
      min-width: 100px;
      font-size: 13px;
    }

    .el-form-item__label:before {
      padding-right: 4px;
      margin: 0;
      box-sizing: border-box;
    }

    .el-form-item__content {
      width: 100%;

      p {
        font-size: 13px;
        line-height: 10px;
        height: 20px;
        border-bottom: 1px solid #ccc;
      }

      .el-form-item__error {
        top: 36px
      }

      .el-input {
        width: 100%;
      }

      .el-select {
        width: 100%;
      }

      .el-textarea {
        width: 100%;
      }

      .el-cascader {
        width: 100%;
      }

      .el-date-editor {
        width: 100%;
      }

      .el-input--suffix .el-input__inner {
        padding-right: 15px;
      }
    }
  }

  //控制最后一列靠左
  .el-form-item:last-of-type {
    margin-right: auto;
  }
}

//纯表单2 页面
.ipt2 {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  //justify-content: space-between;
  flex-wrap: wrap;
  //padding: 10px 10px 0 10px;
  padding-top: 10px;

  .el-form-item {
    margin: 0;
    width: 265px;
    //min-width: 20%;
    height: 50px;
    display: flex;
    //align-items: center;
    justify-content: center;

    .el-form-item__label {
      width: 105px;
      font-size: 13px;
    }

    .el-form-item__label:before {
      padding-right: 4px;
      margin: 0;
      box-sizing: border-box;
    }

    .el-form-item__content {
      width: calc(100% - 105px);

      p {
        font-size: 12px;
        line-height: 10px;
        height: 20px;
        border-bottom: 1px solid #ccc;
      }

      .el-form-item__error {
        top: 36px
      }

      .el-select {
        width: 100%;
      }

      .el-textarea {
        width: 100%;


        .el-textarea__inner {
          padding: 0 15px;
          height: 28px;
        }
      }

      .el-cascader {
        width: 100%;

        .el-input--suffix .el-input__inner {
          padding-right: 15px;
          padding-left: 15px;
        }
      }

      .el-date-editor {
        width: 100%;

        .el-input__icon {
          width: 20px;
        }

        .el-input--suffix .el-input__inner {
          padding-right: 15px;
          padding-left: 25px;
        }
      }

      .el-input--suffix .el-input__inner {
        padding-right: 15px;
        padding-left: 25px;
      }
      .el-input__inner {
        padding-right: 15px;
        padding-left: 25px;
      }
    }
  }

  .money {
    width: 265px;

    .el-form-item__label {
      width: 130px;
      //font-size: 13px;
    }

    .el-form-item__content {
      width: 130px;

      .el-input-number--mini {
        width: 130px;
      }
    }
  }

  .receiptAddress, .remarks, .address {
    width: 530px;
    .el-form-item__content{
      .el-textarea{
        .el-textarea__inner{
          padding:3px 15px 0 25px;
          //color: #606266;
          font-family: Helvetica Neue,Helvetica,PingFang SC,Hiragino Sans GB,Microsoft YaHei,SimSun,sans-serif;
        }
      }
    }
  }
}
 .remarks {
  .el-form-item__content{
    .el-textarea{
      .el-textarea__inner{
        padding:3px 15px 0 25px;
        //color: #606266;
        font-family: Helvetica Neue,Helvetica,PingFang SC,Hiragino Sans GB,Microsoft YaHei,SimSun,sans-serif;
      }
    }
  }
}
