//表格单元格内容居中
.el-table {
  width: 100%;

  table {
    .cell {
      text-align: center;
      padding: 0 10px;
    }
  }
}

//显示合计行滚动条，隐藏表体滚动条，滚动条移到合计行下方
.el-table--scrollable-x .el-table__body-wrapper {
  overflow-x: hidden !important;
  z-index: 2 !important;
}

.el-table__footer-wrapper {
  overflow-x: auto;
  border-top: 1px solid #f4f4f4;
}

//弹框标题底部样式
.dialog-style {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;


  .el-dialog {
    //height: 95%;
    margin-top: 0;
    max-height: 90%;
    //min-height: 20%;
    min-height: 5%;
    position: relative;
    //margin: 0 !important;
    display: flex;
    flex-direction: column;
    overflow: auto;

    & .el-dialog__header {
      background: rgba(204, 204, 204, 0.18);
      width: 100%;
      padding: 10px;

      .el-dialog__headerbtn {
        top: 15px;
      }

      & .el-dialog__title {
        font-weight: 600;
        font-size: 14px;
        color: #333;
        letter-spacing: 1px;
        opacity: .8;
      }
    }

    & .el-dialog__body {
      padding: 10px 15px;
      font-size: 12px;
      min-height: 130px;
      overflow: auto;
      //overflow: scroll;
      //background-color: red;
    }

    & .el-dialog__footer {
      background: rgba(204, 204, 204, 0.18);
      width: 100%;
      padding: 10px;
    }

    & .el-button + .el-button {
      margin-left: 6px;
    }
  }
}
