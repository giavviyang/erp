//明细弹窗
.detailDialog {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;

  .el-dialog {
    height: calc(60vh);

    & .el-dialog__header {
      background: rgba(204, 204, 204, 0.18);
      width: 100%;
      padding: 10px;

      .el-dialog__headerbtn {
        top: 15px;
      }

      & .el-dialog__title {
        font-weight: 600;
        font-size: 14px;
        color: #333;
        letter-spacing: 1px;
        opacity: .8;
      }
    }

    .el-dialog__body {
      height: calc(100% - 50px);
      overflow: auto;
      display: flex;
      flex-direction: column;

      .detailsInfo {
        //min-height: 30px;
        //max-height: 200px;
        height: 100px;
        display: flex;
        //justify-content: space-around;
        flex-wrap: wrap;
        width: 95%;
        margin: 10px auto;
        overflow: auto;

        p {
          min-width: 300px;
          margin: 5px 10px;
          font-size: 13px;
          color: #606266;
          font-weight: 700;
        }
      }

      .detailsTable {
        height: calc(100% - 120px);
        padding: 0 10px 10px 10px;
        box-sizing: border-box;

        .table {
          height: 100%;
          border-bottom: 1px solid #dfe6ec;
        }

        .page {
          display: none;
        }
      }

    }
  }
}

//打包、退货新增统一样式
.publicAddDialog {
  .el-dialog {
    height: calc(75vh);
    overflow: auto;

    .el-dialog__body {
      height: calc(100% - 90px);
      display: flex;
      flex-direction: column;

      .addInfo {
        max-height: 200px;
        min-height: 120px;
        border-radius: 15px;
        box-shadow: 0 1px 5px 0 rgba(0, 0, 0, 0.3);

        .title {
          height: 30px;
          padding-left: 40px;
          margin-bottom: 0 !important;
        }

        .title1 {
          height: 30px;
          padding-left: 40px;
          //margin-bottom: 0 !important;
        }

        .addInfoContainer {
          height: calc(100% - 30px);
          flex-grow: 1;
          overflow: auto;
          background: rgba(100, 200, 188, 0.05);
          border: 1px solid rgba(100, 200, 188, 0.5);
          border-bottom-left-radius: 15px;
          border-bottom-right-radius: 15px;

          .el-form-item {
            width: 380px;
          }

          .remarks {
            width: 760px;
          }
        }
      }

      .btnTable {
        flex-grow: 1;
        min-height: 220px;

        .addTable {
          height: calc(100% - 48px);

          .table {
            height: 100%;
            border-right: 1px solid #dfe6ec;
          }

          .page {
            display: none;
          }
        }
      }
    }
  }
}

